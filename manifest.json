{
    "name":"Choose color",
    "description":"A extension to choose Color",
    "version":"1.0" ,
    "manifest_version":3,
    "background":{
        "service_worker":"background.js"
    },
    "permissions":[
        "storage",
       "activeTab",
       "scripting"
    ],
    "options_page":"options.html",
    "action":{
        "default_popup":"popup.html"
    },
    "icons":{
        "16":"/images/color-wheel-16.png",
        "32":"/images/color-wheel-32.png",
        "48":"/images/color-wheel-48.png"
    }
}